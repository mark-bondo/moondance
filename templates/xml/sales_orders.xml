<?xml version="1.0" encoding="utf-8" ?>
<OrderQueryRequest>
    <Credentials>
        <AccountName>moondance</AccountName>
        <Key>%(key)s</Key>
    </Credentials>
    <OrderUpdRange>
        <OrderUpdStart>
            <DateTime>
                <Date>%(start_date)s</Date>
                <Time>00:00</Time>
            </DateTime>
        </OrderUpdStart>
        <OrderUpdEnd>
            <DateTime>
                <Date>%(end_date)s</Date>
                <Time>23:59</Time>
            </DateTime>
        </OrderUpdEnd>
    </OrderUpdRange>
    %(page_number)s
</OrderQueryRequest>